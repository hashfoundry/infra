# DigitalOcean Spaces Configuration
# Скопируйте этот файл в .env и заполните реальными значениями

# DigitalOcean Spaces Access Key ID
DO_SPACES_ACCESS_KEY_ID=your_access_key_here

# DigitalOcean Spaces Secret Access Key  
DO_SPACES_SECRET_ACCESS_KEY=your_secret_key_here

# DigitalOcean Spaces Region
DO_SPACES_REGION=nyc3

# DigitalOcean Spaces Bucket Name
DO_SPACES_BUCKET=hashfoundry-ml-models

# DigitalOcean Spaces Endpoint URL
DO_SPACES_ENDPOINT_URL=https://nyc3.digitaloceanspaces.com

# Model Storage Path
MODEL_STORAGE_PATH=iris-classifier/v1/

# Local Model Files
LOCAL_MODEL_PATH=ha/ml/iris-classifier/model.pkl
LOCAL_METADATA_PATH=ha/ml/iris-classifier/metadata.json
